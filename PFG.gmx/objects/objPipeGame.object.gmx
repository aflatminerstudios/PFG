            <string>/// Move cursor via player controls

if (scrCheckButtonPressed(player, LEFT)) {
  if(cursor.x &gt;= x + cursor.sprite_width)
    cursor.x -= cursor.sprite_width;
}

if (scrCheckButtonPressed(player, RIGHT)) {
  if(cursor.x &lt; (x + fieldWidth) - cursor.sprite_width)
    cursor.x += cursor.sprite_width;
}

if (scrCheckButtonPressed(player, UP)) {
  if(cursor.y &gt;= y + cursor.sprite_height)
    cursor.y -= cursor.sprite_height;
}

if (scrCheckButtonPressed(player, DOWN)) {
  if(cursor.y &lt; (y + fieldHeight) - cursor.sprite_height)
    cursor.y += cursor.sprite_height;
}

