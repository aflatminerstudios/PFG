    //Stop column
    if (position_meeting(x, y + 1, objColBlockSettled) || y = host.y + host.fieldHeight) {
    
        if (y &lt;= (host.y + 10)) {
            host.gameAlive = false;            
            show_debug_message("Lost block game");
        }    
        with (block1) {
            instance_change(objColBlockSettled, false);
        }
        with (block2) {
            instance_change(objColBlockSettled, false);
        }
        with (block3) {
            instance_change(objColBlockSettled, false);
        }
        instance_destroy();
       
        
    } else {
    
        if (scrCheckButtonPressed(host.player, LEFT)) {
            if (x &gt; host.x &amp;&amp; place_free(x - 30, y)) {
                x -= 32;
            }
        }
        
        if (scrCheckButtonPressed(host.player, RIGHT)) {
            if (x &lt; host.x + host.fieldWidth - 32 &amp;&amp; place_free(x + 30, y)) {
                x += 32;
            }
        }
        
        if (scrCheckButton(host.player, DOWN)) {
            host.currentSpeed = host.baseSpeed * 3;
        } else {
          host.currentSpeed = host.baseSpeed;
        }
    }   
}
