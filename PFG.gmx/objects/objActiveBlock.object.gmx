            <string>///Block Movement

if (host != noone) {       
    if (position_meeting(x, y + 1, objBlock) || y = host.y + host.fieldHeight) {
        
        instance_change(objBlock, false);
        
    } else {
        if (keyboard_check_pressed(vk_left)) {
            if (x &gt; host.x) {
                x -= 32;
            }
        }
        
        if (keyboard_check_pressed(vk_right)) {
            if (x &lt; host.x + host.fieldWidth - 32) {
                x += 32;
            }
        }
        
        if (keyboard_check(vk_down)) {
            host.currentSpeed = host.baseSpeed * 3;
        }
        
        if (keyboard_check_released(vk_down)) {
            host.currentSpeed = host.baseSpeed;
        }
    }   
}
