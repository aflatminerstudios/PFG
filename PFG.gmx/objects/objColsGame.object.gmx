var hostId = self.id;

var matched;
matched[0] = -100;

with (objColBlockSettled) {
    if(host == hostId)
      matched = scrColsCheckMatches(self.id, matched);
}


if (is_array(matched)) {
    var h = array_length_1d(matched);
    
        
