matched[0] = -100;


with (objColBlockSettled) {
    if(host == hostId)
      matched = scrColsCheckMatches(self.id, matched);
}


if (is_array(matched)) {
    var h = array_length_1d(matched);
            
    for (i = 0; i &lt; h; ++i) {
        var blocks = matched[i];
        
        //Check for order match
        if (is_array(blocks) &amp;&amp; order1 != noone &amp;&amp; instance_exists(order1) 
          &amp;&amp; instance_exists(blocks[0]) &amp;&amp; order1.color == blocks[0].color) {
            matchedOrder(self);
        }
        
        for (j = 0; j &lt; array_length_1d(blocks); ++j) {
            var block = blocks[j];
            with (block) {
                instance_destroy();
            }
        }
    }
}
